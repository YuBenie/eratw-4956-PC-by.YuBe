	ELSEIF TALENT:恋慕
		IF PALAM:欲情 > PALAMLV:4 && RAND:2 == 0
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「吮吸、啊、舔舐、咕噗、啊啊HPH ……噗啊、嗯、咕……呼……HPH」","L"
					PRINTFORML 交织舌头，贪婪地探索彼此口腔
					PRINTFORMW 分开嘴唇时，混合的唾液流入，妖梦满足地咽下，发出满足的喉音
				CASE 1
					CALL HPH_PRINT,@"「呼、嗯嗯、嗯、啊噗HPH 吮吸、舔舐、吮吸……HPH」","L"
					PRINTFORMW 妖梦咬住%CALLNAME:MASTER%的舌尖，引导它进入自己的口腔
				CASE 2
					CALL HPH_PRINT,@"「吮吸、啊、嗯、舔舐HPH 噗噗、嗯、啊、嗯……HPH」","L"
					CALL HPH_PRINT,@"「啊啊、还不够……HPH 啊、吞嚼、嗯、呜……HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「啊噗、吮吸、嗯嗯、啊HPH 嗯嗯……哈、嗯嗯HPH」","L"
					PRINTFORMW 妖梦陶醉于%CALLNAME:MASTER%的吸吮般吻，热情回应
				CASE 1
					CALL HPH_PRINT,@"「嗯嗯、呼、吮吸HPH 呼、噗、嗯、嗯嗯……HPH」","L"
					PRINTFORML 在接吻时，用舌尖沿着张开的嘴唇轻轻滑动
					PRINTFORMW 妖梦被挑逗的动作弄得扭动身体，显得不舒服
				CASE 2
					CALL HPH_PRINT,@"「呼、嗯嗯……HPH 嗯、呼……啊、哈HPH」","L"
					CALL HPH_PRINT,@"「温柔的吻，我喜欢……HPH 啊、嗯嗯、呼啊、嗯嗯HPH」","W"
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		PRINTDATAW
			DATAFORM 「嗯噗、嗯嗯……呜呜……！」
			DATAFORM 「嗯、咕……呼啊、嗯……」
			DATAFORM 「咕、再次……嗯嗯……！」
		ENDDATA
	ENDIF
ENDIF
RETURN 1


