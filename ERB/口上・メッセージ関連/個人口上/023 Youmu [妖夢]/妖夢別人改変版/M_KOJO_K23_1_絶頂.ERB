								CALL HPH_PRINT,@"「……感觉能顺利服务，我很开心HPP」","W"
							CASE 1
								PRINTFORML 「嗯哼……哎呀，好大的量……」
								PRINTFORMW 「视觉上看到会有更大的成就感……」
						ENDSELECT
					ENDIF
				ELSE
				;口からダラダラとこぼれでている
					SELECTCASE RAND:(2 + (ABL:精液中毒 >= 5) * 3)
						CASE 0
							PRINTFORML 「嗯啊，阿普……这、这么……」
							CALL HPH_PRINT,@"「非常抱歉，我做了失礼的事……HPP」","W"
						CASE 1
							PRINTFORML 「嗯嗯，呜……噗啊……！」
							PRINTFORMW 「哎，一口气喝不下呢……嘿咻。」
						CASE 2
							PRINTFORML 「哎呀，全洒出来了……」
							CALL HPH_PRINT,@"「好不容易才拿出来的，真是浪费……啾、噜、嘟，HPP」","W"
						CASE 3
							CALL HPH_PRINT,@"「唔、呼……洒出来的都是我，不打扫一下不行……HPP」","L"
							PRINTFORMW 妖夢用手指轻轻捏起那混浊的液体，犹如品尝美酒般缓缓吸吮……
						CASE 4
							PRINTFORML 「呼……虽然知道又臭又难喝，粘在喉咙上难受……」
							CALL HPH_PRINT,@"看着这个，我好渴啊……噜、嗯……咕噜咕噜，HPP」","W"
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ELSE
		IF (EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3) && EXP:精飲経験 >= 20
			IF ABL:精液中毒 >= 5 && RAND:3 == 0
				PRINTFORML 「嗯……唔……苦……」
				PRINTFORMW （虽然难喝，却停不下来……）
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「唔……唔……一如既往的苦涩味道……」
						PRINTFORMW （对味道本身已经习惯了……）
					CASE 1
						PRINTFORML 「呃……咕……嗯……」
						PRINTFORMW 「……一旦冷却下来，周围不就变得更脏了吗？」
				ENDSELECT
			ENDIF
		ELSE
			IF TCVAR:TARGET:精飲経験 == 0
				PRINTFORML 「咕、呜！？ ...唔嗯、嘿呵、嘿呵」
				PRINTFORMW 「这是什么...你竟然说要喂我，你认真吗...」
			ELSEIF TCVAR:TARGET:精飲経験 < 20
				PRINTFORML 「嘿……嗯……首先……」
				PRINTDATAW
					DATAFORM （哎呀，只能说是糟糕透顶的感觉...……）
					DATAFORM （我想即使通过口述也要喂给他喝...啊...）
				ENDDATA
			ELSE
				IF ABL:精液中毒 >= 5 && RAND:3 == 0
					PRINTFORML 「咕……咕噜……那、那是什么眼神？」
					PRINTFORMW 「被那样黏黏糊糊的盯着，厌恶感也会麻痹的……」
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「嘻嘻……这、这么多是从哪里来的……？」
						CASE 1
							PRINTFORML 「嗯……嗯嗯……等、等等……」
							PRINTFORMW （光用嘴说根本收不住……）
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF
