;==================================================
;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K107_140
CALL M_KOJO_MESSAGE_COM_K107_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K107_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			CALL HPH_PRINT,@"「嗯嗯…♪啾啾…HPH 嗯…舒服吗…？」","L"
			PRINTFORMDL 刺激还不够——%CALLNAME:MASTER%抓住清兰的兔耳，强行拉近她的头，将肉棒塞入她的喉咙。
			PRINTFORML 「嗯嘟！？嗯咕……！嗯、嗯嗯！！」
			PRINTFORMDW 感受到喉咙被阴茎挤压的快感，%CALLNAME:MASTER%开始主动动作，追求更多快感…！
		ENDIF
		RETURN 1
	ENDIF
	;基本设定
	;时姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		;已射精
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「嗯！？嗯呜！！嗯嗯嗯！」
					PRINTFORMDL 告诉她"我要射了"，清兰摇着头似乎在向%CALLNAME:MASTER%求情。
					PRINTFORMDL 她似乎在说不要射精…%CALLNAME:MASTER%无视，继续将阴茎射精进她的喉咙。
					PRINTFORML 「嗯咕……！咕…！嗯……咕……」
					PRINTFORMDW 拔出阴茎后，清兰倒在地上昏迷过去了……
				CASE 1
					CALL HPH_PRINT,@"「嗯咕噜噢噢噢……HPH…嗯咕…咕…咕咕…HPH…噢…咕哈！！」","L"
					PRINTFORMDL 射精进清兰的喉咙，但显然她感到不适，%CALLNAME:MASTER%用力将阴茎从口中抽出。
					PRINTFORML 「噢额呃…！呵呼！呵呼…！咕呼…！」
					PRINTFORML 「哈啊…哈啊…为什么这样…粗暴呢……？」
					PRINTFORMDW 清兰用害怕的眼神看着%CALLNAME:MASTER%……
				CASE 2
					;抱歉，太长了
					CALL HPH_PRINT,@" 「噢咕！嗯！嗯嗯嗯HPH 嗯咕噜HPH」","L"
					PRINTFORMDL 在深入喉咙的同时，告诉清兰"我快射了"。
					PRINTFORML 「！？―嗯！咕哈！%CALLNAME:MASTER%等等！你突然怎么了！？」
					PRINTFORMW 「像平常一样……温柔点呀……！」
					PRINTFORMDL 或许有些过头了，清兰坐在地上哭泣起来…"对不起"，%CALLNAME:MASTER%轻轻抚摸清兰的头…
					CALL HPH_PRINT,@"「……%CALLNAME:MASTER%…好的♪…我原谅你了HPH」","W"
					PRINTFORML 「―啊！？停、停下来！嗯咕……！」
					PRINTFORMDL %CALLNAME:MASTER%抓住清兰放松的瞬间，再次将即将射精的阴茎塞入她的喉咙，将精液射入其中。
					CALL HPH_PRINT,@"「嗯咕…HPH 嗯咕…！咕哈……咕咕…HPH」","L"
					PRINTFORMDW 喝下精液后，清兰像断了线的玩偶一样软倒在地，静静地流泪……
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「嗯咕嗯……！噢呜……啾…嘟…噢」
					PRINTFORML （我，我在做什么来着…？头…有点晕晕的……）
					PRINTFORMDW 清兰被抓住%TEXTR("頭/兔耳")%剧烈摇晃，意识似乎有些模糊。
				CASE 1
					PRINTFORML 「嗯咕呜！嗯咕咕！噢呜！嗯咕！嗯咕嗯嗯！」
					PRINTFORMDW 抓住清兰的%TEXTR("頭/兔耳")%，多次插入喉咙。
				CASE 2
					PRINTFORML 「噢呜……噢呜！嗯嗯…嗯嘟！额呃呜呜！」
					PRINTFORMW 清兰眼角泛泪，忍受着%CALLNAME:MASTER%的冲击……
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

